{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint/recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": { "no-unused-vars": "warn" }
}
